  <properties>
	<property key="licensekey" type="microflow" required="true">
		<caption>License key</caption>
      	<category>Widget</category>
      	<description>Let this  microflow return your license key. If you deploy your application on multiple locations 
      	    you can use a constant holding the license key.</description>
		<returnType type="String"></returnType>
	</property>
      	
    <property key="caption" type="string" required="true" >
        <caption>Caption</caption>
        <category>Fixed</category>
        <description>Caption displayed on the button to make the payment. Overwritten by dynamic variant if applied.</description>
    </property>
	<property key="currency" type="string" required="true" >
        <caption>Currency</caption>
        <category>Fixed</category>
        <description>Payment currency: Google on "Paypal Currency codes". Overwritten by dynamic variant if applied.</description>
    </property>
	<property key="business" type="string" required="true" >
        <caption>Business</caption>
        <category>Fixed</category>
        <description>Paypal e-mail address of the seller. Overwritten by dynamic variant if applied.</description>
    </property>
	<property key="itemname" type="string" required="true" >
        <caption>Cart item</caption>
        <category>Fixed</category>
        <description>Cart item description (shown on paypal receipt). Overwritten by dynamic variant if applied.</description>
    </property>
    <property key="price" type="string" required="true" >
        <caption>Price</caption>
        <category>Fixed</category>
        <description>The price that have to be paid. Overwritten by dynamic variant if applied.</description>
    </property>
    <property key="url" type="string" required="true" >
        <caption>Return url</caption>
        <category>Fixed</category>
        <description>The URL to return to after payment. Overwritten by dynamic variant if applied.</description>
    </property>
	<property key="target" type="enumeration" defaultValue="_blank" required="true" >
        <caption>Target</caption>
        <category>Fixed</category>
        <description>Target screen to open Paypal environment.</description>
        <enumerationValues>
			<enumerationValue key="_blank">New browser page</enumerationValue> 
			<enumerationValue key="_self">Same browser page</enumerationValue> 
			<enumerationValue key="_parent">Parent</enumerationValue> 
			<enumerationValue key="_top">Top</enumerationValue> 
		</enumerationValues>
    </property>
    <property key="captiond" type="attribute" required="false">
		<caption>Caption (d)</caption>
		<category>Dynamics</category>
		<description>Caption displayed on the button to make the payment.</description>
		<attributeTypes>
			<attributeType name="String"/> 
		</attributeTypes>
	</property>
	<property key="currencyd" type="attribute" required="false">
		<caption>Currency (d)</caption>
		<category>Dynamics</category>
		<description>Payment currency: Google on "Paypal Currency codes". </description>
		<attributeTypes>
			<attributeType name="String"/> 
		</attributeTypes>
	</property>
	<property key="businessd" type="attribute" required="false">
		<caption>Business (d)</caption>
		<category>Dynamics</category>
		<description>Paypal e-mail address of the seller. </description>
		<attributeTypes>
			<attributeType name="String"/> 
		</attributeTypes>
	</property>
	<property key="itemnamed" type="attribute" required="false">
		<caption>Cart item (d)</caption>
		<category>Dynamics</category>
		<description>Cart item description (shown on paypal receipt).</description>
		<attributeTypes>
			<attributeType name="String"/> 
		</attributeTypes>
	</property>
	<property key="priced" type="attribute" required="false">
		<caption>Price (d)</caption>
		<category>Dynamics</category>
		<description>The price that have to be paid.</description>
		<attributeTypes>
			<attributeType name="Currency"/> 
			<attributeType name="Float"/> 
			<attributeType name="Integer"/> 
		</attributeTypes>
	</property>
	<property key="urld" type="attribute" required="false" >
        <caption>Return url (d)</caption>
        <category>Dynamics</category>
        <description>The URL to return to after payment.</description>
        <attributeTypes>
			<attributeType name="String"/> 
		</attributeTypes>
    </property>
	<property key="reference" type="attribute" required="true">
		<caption>Tracking reference</caption>
		<category>Dynamics</category>
		<description>Reference posted back by paypal after completing the transaction. </description>
		<attributeTypes>
			<attributeType name="String"/> 
			<attributeType name="Currency"/> 
			<attributeType name="Float"/> 
			<attributeType name="Integer"/> 
			<attributeType name="AutoNumber"/> 
		</attributeTypes>
	</property>
	<property key="afterclickaction" type="microflow" required="false">
		<caption>Onclick action</caption>
		<category>Dynamics</category>
		<description>Action which is called when the user clicks on the button. For example: save order and set status to completed and return to home screen.</description>
		<returnType type="Void"/>
	</property>
  </properties>
  
</widget>